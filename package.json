{
  "name": "qrgenerator",
  "version": "1.0.0",
  "description": "QR code generator for Czech payment system",
  "main": "dist/server/server.js",
  "scripts": {
    "build": "npm run build:server && npm run build:app",
    "build:server": "tsc -p tsconfig.json",
    "build:app": "cd app && vite build && if not exist ..\\dist\\app mkdir ..\\dist\\app && xcopy /E /I /Y build ..\\dist\\app",
    "start": "node dist/server/server.js",
    "dev": "ts-node server/server.ts",
    "test": "ts-node server/test.ts",
    "dev:app": "cd app && vite"
  },
  "author": "Vilém Ťulák Jeniš",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^5.0.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
