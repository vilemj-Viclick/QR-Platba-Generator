{
  "name": "qrgenerator",
  "version": "1.0.0",
  "description": "QR code generator for Czech payment system",
  "main": "dist/server/server.js",
  "scripts": {
    "build": "npm run build:packages && npm run build:server && npm run build:app",
    "build:packages": "cd packages/qr-platba-generator && npm install && npm run build",
    "build:server": "tsc -p tsconfig.json",
    "build:app": "cd app && vite build && if not exist ..\\dist\\app mkdir ..\\dist\\app && xcopy /E /I /Y build ..\\dist\\app",
    "start": "node dist/server/server.js",
    "dev": "ts-node server/server.ts",
    "test:server": "ts-node server/test-server.ts",
    "test": "vitest run",
    "dev:app": "cd app && vite"
  },
  "author": "Vilém Ťulák Jeniš",
  "license": "MIT",
  "dependencies": {
    "ajv": "8.17.1",
    "axios": "1.11.0",
    "body-parser": "1.20.3",
    "express": "4.21.2",
    "qr-platba-generator": "file:packages/qr-platba-generator",
    "qrcode": "1.5.4",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/express": "4.17.23",
    "@types/node": "18.19.121",
    "@types/qrcode": "1.5.5",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react": "4.7.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vite": "5.4.19",
    "vitest": "3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
